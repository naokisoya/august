<!DOCTYPE html>
<html>
<head>
  <title>August</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="header">
	<%= link_to('Home', root_path) %>
	<%= link_to('Users', users_path) %>
</div>
<%= yield %>
<hr />
<% if user_signed_in? %>
	<%= current_user.email %>
	<%= link_to('Profile',current_user) %>
<%= end %>
<div id="footer">
	<%= link_to "Home", root_path %>
	<%= link_to "About", about_path %>
	<%= link_to "Help", help_path %>
</body>
</html>
